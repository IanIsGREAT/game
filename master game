		printf("\nYou got Nick\n");
	}
	else if(chance >= 84 && chance <= 92)
	{
		printf("\nYou got Ashraf\n");
	}
	else
	{
		printf("\nYou got Michael\n");
	}

	return(0);

}
int finished(void)
{
    for (;;) {

        std::cout << "\b\b\b\b\b\b\b\b\b\b\b\b\bFinished" << std::flush;
        Sleep(100);
        std::cout << "\b\b\b\b\b\b\b\b\b\b\b\b\bfInished" << std::flush;
        Sleep(100);
        std::cout << "\b\b\b\b\b\b\b\b\b\b\b\b\bfiNished" << std::flush;
        Sleep(100);
        std::cout << "\b\b\b\b\b\b\b\b\b\b\b\b\bfinIshed" << std::flush;
        Sleep(100);
        std::cout << "\b\b\b\b\b\b\b\b\b\b\b\b\bfiniShed" << std::flush;
        Sleep(100);
        std::cout << "\b\b\b\b\b\b\b\b\b\b\b\b\bfinisHed" << std::flush;
        Sleep(100);
        std::cout << "\b\b\b\b\b\b\b\b\b\b\b\b\bfinishEd" << std::flush;
        Sleep(100);
        std::cout << "\b\b\b\b\b\b\b\b\b\b\b\b\bfinisheD" << std::flush;
        Sleep(100);
    }


    return 0;
}    
int UserInput(int min, int max)
{
 int c;

    do
 {
  scanf("%i", &c);

  if(c < min || c > max)
   printf("Invalid entry, please try again: ");
 }while(c < min || c > max);

 return(c);

 }
int characterstats(void)
{
	//mike   str 116 int 130 soc 148 spd 144 hp 96
	//ashraf str 134 int 108 soc 120 spd 148 hp 106
	//nick   str 120 int 110 soc 128 spd 138 hp 120
	//bryan  str 118 int 102 soc 136 spd 112 hp 124
	//ian    str 104 int 148 soc 106 spd 114 hp 94
	//kevin  str 148 int 50  soc 124 spd 60  hp 148
	//zach   str 90  int 94  soc 68  spd 84  hp 86
	//sean f str 80  int 72  soc 92  spd 116 hp 50
	//dylan  str 70  int 122 soc 50  spd 74  hp 70
	//sean c str 50  int 64  soc 76  spd 50  hp 88
	return(0);
}
int singleplayer(void)
{
	int choice, choice2, choice3, choice4, damage, damage2, strength, intellect, hitpoints, strength2, intellect2, hitpoints2, defense, defense2,  stat, stat2, predamage, predamage2, lvl;
    float per, per2;
		int ai, social, social2, ain;
    
	do
	{
	choice = characterselection();
	

	if(choice == 1)
	{
		
		strength = 120;
		intellect = 80;
		social = 100;
		hitpoints = 110;
		printf("Srength = %i, Intellect = %i, Social = %i\n", strength, intellect, social);
	}
	else if(choice == 2)
	{
		
		strength = 70;
		intellect = 130;
		social = 100;
		hitpoints = 90;
		printf("Srength = %i, Intellect = %i, Social = %i\n", strength, intellect, social);
	}
		printf("\nA GIANT APPEARS!!!!!\n");

		strength2 = 100;
		intellect2 = 50;
		social2 = 75;
		hitpoints2 = 75;

		do
		{
		printf("\nChoose your attack: 1 = Strength 2 = Intellect 3 = Social : ");

	choice2 = UserInput(1,3);
	if(choice2 == 1 && hitpoints > 0)
	{
		predamage = strength / 5;
		stat = strength - social2;
		per = stat * 0.01;
		defense = predamage * per;
		damage = predamage + defense + 10;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nGiant defends with social! The attack is super effective\n");
	}
	else  if(choice2 == 2 && hitpoints > 0)
	{
		predamage = intellect / 5;
		stat = intellect - strength2;
		per = stat * 0.01;
		defense = predamage * per;
		damage = predamage + defense + 10;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nGiant defends with strength! The attack is super effective\n");
	}

	else if(choice2 == 3 && hitpoints > 0)
	{
		predamage = social / 5;
		stat = social - intellect2;
		per = stat * 0.01;
		defense = predamage * per;
		damage = predamage + defense + 10;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nGiant defends with intellect! The attack is super effective\n");
	}

	else
	{
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nYou are dead, no damage done\n");
		lvl = 1;
		break;
	}

	printf("\n%i Damage Dealt", damage);

		hitpoints2 = hitpoints2 - damage;
	if(hitpoints2 < 0)
	{
		hitpoints2 = 0;
	}
	printf("\nGiant health %i\n\n\n\n\n\n\n\n", hitpoints2);

	ai = rng100();

	if(ai >= 0 && ai <= 32 && hitpoints2 > 0)
	{
		predamage2 = strength2 / 5;
		ain = 1;
	}

	else if(ai >= 33 && ai <= 66 && hitpoints2 > 0)
	{
		predamage2 = intellect2 / 5;
		ain = 2;
	}

	else if(ai >= 67 && ai <= 100 && hitpoints2 > 0)
	{
		predamage2 = social2 / 5;
		ain = 3;
	}

	else
	{
		printf("\nYou have killed the giant\n");
		lvl = 2;
		break;
	}

	printf("The giant has attacked, how do you defend? : ");
	choice3 = UserInput(1,3);
	if(choice3 == 1 && ain == 1 && hitpoints2 > 0)
	{
		stat = strength2 - strength ;
		per = stat * 0.01;
		defense2 = predamage2 * per;
		damage2 = predamage2 + defense2;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
	}
	else if(choice3 == 2 && ain == 1 && hitpoints2 > 0)
	{
		stat = strength2 - intellect;
		per = stat * 0.01;
		defense2 = predamage2 * per;
		damage2 = predamage2 + defense2 - 10;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nThe attack is not very effective\n");
		
	}
	else if(choice3 == 3 && ain == 1 && hitpoints2 > 0)
	{
		stat = strength2 - social;
		per = stat * 0.01;
		defense2 = predamage2 * per;
		damage2 = predamage2 + defense2 + 10;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nThe attack is super effective\n");
		
		
	}
	else if(choice3 == 1 && ain == 2 && hitpoints2 > 0)
	{
		stat = intellect2 - strength;
		per = stat * 0.01;
		defense2 = predamage2 * per;
		damage2 = predamage2 + defense2 + 10;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nThe attack is super effective");
		
	}
	else if(choice3 == 2 && ain == 2 && hitpoints2 > 0)
	{
		stat = intellect2 - intellect;
		per = stat * 0.01;
		defense2 = predamage2 * per;
		damage2 = predamage2 + defense2;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
		
	}
	else if(choice3 == 3 && ain == 2 && hitpoints2 > 0)
	{
		stat = intellect2 - social;
		per = stat * 0.01;
		defense2 = predamage2 * per;
		damage2 = predamage2 + defense2 - 10;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nThe attack is not very effective");
		
	}
	else if(choice3 == 1 && ain == 3 && hitpoints2 > 0)
	{
		stat = social2 - strength;
		per = stat * 0.01;
		defense2 = predamage2 * per;
		damage2 = predamage2 + defense2 - 10;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nThe attack is not very effective");
		
	}
	else if(choice3 == 2 && ain == 3 && hitpoints2 > 0)
	{
		stat = social2 - intellect;
		per = stat * 0.01;
		defense2 = predamage2 * per;
		damage2 = predamage2 + defense2 + 10;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nThe attack is super effective");
		
	}
	else if(choice3 == 3 && ain == 3 && hitpoints2 > 0)
	{
		stat = social2 - social;
		per = stat * 0.01;
		defense2 = predamage2 * per;
		damage2 = predamage2 + defense2;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
		
	}
	
	printf("\n%i Damage Dealt", damage2);

		hitpoints = hitpoints - damage2;
	if(hitpoints < 0)
	{
		hitpoints = 0;
	}
	printf("\nYour health %i\n\n\n\n\n\n\n\n", hitpoints);



	}while(hitpoints > 0 && hitpoints2 > 0);

		if(hitpoints2 <= 0)
	{
		printf("\nYOU WIN!!!!!\n");
	}
	else if (hitpoints <= 0)
	{
		printf("\nGAME OVER!!!!!\n");
	}
		if(lvl == 1)
		{
	printf("\nWould You like to play again: 1 - Yes 2 - No: ");
	choice4 = UserInput(1,2);
		}
		else if(lvl == 2)
		{
			printf("\nProceed to the next level?: 1 - No 2 - Yes: ");
	choice4 = UserInput(1,2);
		}

	}while(choice4 == 1);



	//lvl 2




	if(lvl == 2)
	{
		do
	{
		printf("LEVEL 2\n");
	choice = characterselection();
	

	if(choice == 1)
	{
		
		strength = 120;
		intellect = 80;
		social = 100;
		hitpoints = 110;
		printf("Srength = %i, Intellect = %i, Social = %i\n", strength, intellect, social);
	}
	else if(choice == 2)
	{
		
		strength = 70;
		intellect = 130;
		social = 100;
		hitpoints = 90;
		printf("Srength = %i, Intellect = %i, Social = %i\n", strength, intellect, social);
	}
		printf("\nA DRAGON APPEARS!!!!!\n");

		strength2 = 100;
		intellect2 = 125;
		social2 = 75;
		hitpoints2 = 100;

		do
		{
		printf("\nChoose your attack: 1 = Strength 2 = Intellect 3 = Social : ");

	choice2 = UserInput(1,3);
	if(choice2 == 1 && hitpoints > 0)
	{   ai = rng100();
	    if(ai >= 0 && ai <= 24)
		{
		predamage = strength / 5;
		stat = strength - social2;
		per = stat * 0.01;
		defense = predamage * per;
		damage = predamage + defense + 10;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nDragon defends with social! The attack is super effective\n");
	}
	else if(ai >= 25 && ai <= 74)
	{
		predamage = strength / 5;
		stat = strength - strength2;
		per = stat * 0.01;
		defense = predamage * per;
		damage = predamage + defense;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nDragon defends with strength!\n");
	}
	else if(ai >= 75 && ai <= 100)
	{
		predamage = strength / 5;
		stat = strength - intellect2;
		per = stat * 0.01;
		defense = predamage * per;
		damage = predamage + defense - 10;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nDragon defends with intellect! The attack is not very effective\n");
	}
	}
	else  if(choice2 == 2 && hitpoints > 0)
	{
		ai = rng100();
	    if(ai >= 0 && ai <= 24)
		{
		predamage = intellect / 5;
		stat = intellect - strength2;
		per = stat * 0.01;
		defense = predamage * per;
		damage = predamage + defense + 10;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nDragon defends with strength! The attack is super effective\n");
	}
	else if(ai >= 25 && ai <= 74)
	{
		predamage = intellect / 5;
		stat = intellect - intellect2;
		per = stat * 0.01;
		defense = predamage * per;
		damage = predamage + defense;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nDragon defends with intellect!\n");
	}
	else if(ai >= 75 && ai <= 100)
	{
		predamage = intellect / 5;
		stat = intellect - social2;
		per = stat * 0.01;
		defense = predamage * per;
		damage = predamage + defense - 10;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nDragon defends with social! The attack is not very effective\n");
	}
	}

	else if(choice2 == 3 && hitpoints > 0)
	{
		ai = rng100();
	    if(ai >= 0 && ai <= 24)
		{
		predamage = social / 5;
		stat = social - intellect2;
		per = stat * 0.01;
		defense = predamage * per;
		damage = predamage + defense + 10;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nDragon defends with intellect! The attack is super effective\n");
	}
	else if(ai >= 25 && ai <= 74)
	{
		predamage = strength / 5;
		stat = social - social2;
		per = stat * 0.01;
		defense = predamage * per;
		damage = predamage + defense;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nDragon defends with social!\n");
	}
	else if(ai >= 75 && ai <= 100)
	{
		predamage = strength / 5;
		stat = social - strength2;
		per = stat * 0.01;
		defense = predamage * per;
		damage = predamage + defense - 10;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nDragon defends with strength! The attack is not very effective\n");
	}
	}

	else
	{
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nYou are dead, no damage done\n");
		lvl = 2;
		break;
	}

	printf("\n%i Damage Dealt", damage);

		hitpoints2 = hitpoints2 - damage;
	if(hitpoints2 < 0)
	{
		hitpoints2 = 0;
	}
	printf("\nDragon health %i\n\n\n\n\n\n\n\n", hitpoints2);

	ai = rng100();

	if(ai >= 0 && ai <= 24 && hitpoints2 > 0)
	{
		predamage2 = strength2 / 5;
		ain = 1;
	}

	else if(ai >= 25 && ai <= 74 && hitpoints2 > 0)
	{
		predamage2 = intellect2 / 5;
		ain = 2;
	}

	else if(ai >= 75 && ai <= 100 && hitpoints2 > 0)
	{
		predamage2 = social2 / 5;
		ain = 3;
	}

	else
	{
		printf("\nYou have slain the dragon\n");
		lvl = 3;
		break;
	}

	printf("The dragon has attacked, how do you defend? : ");
	choice3 = UserInput(1,3);
	if(choice3 == 1 && ain == 1 && hitpoints2 > 0)
	{
		stat = strength2 - strength ;
		per = stat * 0.01;
		defense2 = predamage2 * per;
		damage2 = predamage2 + defense2;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
	}
	else if(choice3 == 2 && ain == 1 && hitpoints2 > 0)
	{
		stat = strength2 - intellect;
		per = stat * 0.01;
		defense2 = predamage2 * per;
		damage2 = predamage2 + defense2 - 10;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nThe attack is not very effective\n");
		
	}
	else if(choice3 == 3 && ain == 1 && hitpoints2 > 0)
	{
		stat = strength2 - social;
		per = stat * 0.01;
		defense2 = predamage2 * per;
		damage2 = predamage2 + defense2 + 10;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nThe attack is super effective\n");
		
		
	}
	else if(choice3 == 1 && ain == 2 && hitpoints2 > 0)
	{
		stat = intellect2 - strength;
		per = stat * 0.01;
		defense2 = predamage2 * per;
		damage2 = predamage2 + defense2 + 10;
		printf("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nThe attack is super effective");
		
